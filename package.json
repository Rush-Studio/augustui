{
  "name": "augustui",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "preinstall": "npx only-allow npm",
    "dev": "next dev --turbo",
    "prebuild": "npm run build:docs && npm run build:registry",
    "build": "next build",
    "build:registry": "npx tsx ./scripts/build-registry.mts",
    "build:docs": "content-collections build",
    "typecheck": "npm run build:docs && tsc --noEmit",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "prepare": "husky && test -f .env.local || cp .env.example .env.local"
  },
  "dependencies": {
    "@next/swc-wasm-nodejs": "13.5.1",
    "@next/third-parties": "^15.2.4",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "jotai": "^2.12.2",
    "lucide-react": "^0.446.0",
    "mixpanel-browser": "^2.62.0",
    "motion": "^12.6.2",
    "next": "^15.2.4",
    "next-themes": "^0.3.0",
    "postcss": "^8.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shadcn": "^2.3.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.6.0",
    "typescript": "5.2.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@content-collections/cli": "^0.1.6",
    "@content-collections/core": "^0.8.2",
    "@content-collections/mdx": "^0.2.2",
    "@content-collections/next": "^0.2.6",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@types/mixpanel-browser": "^2.54.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.2.4",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-tailwindcss": "^3.18.0",
    "husky": "^9.1.7",
    "mdast-util-toc": "^7.1.0",
    "prettier": "^3.5.3",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-code-import": "^1.2.0",
    "remark-gfm": "^4.0.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.3",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0"
  },
  "engines": {
    "node": ">=20.19.0",
    "pnpm": ">=10.8.2"
  }
}
